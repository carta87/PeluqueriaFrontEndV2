@page
@model PeluqueiaStar.Pages_Estelista.EditModel

@{
    ViewData["Title"] = "Edit";
}

<div >
    <br>
    <strong>
        <font face="Comic Sans MS,arial" style="color:indigo" size="6">Editar Estelista</font>
    </strong>
    <br>
   
</div><br><br>


<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Estelista.TarjetaProfesional" class="control-label"></label>
                <input asp-for="Estelista.TarjetaProfesional" class="form-control" />
                <span asp-validation-for="Estelista.TarjetaProfesional" class="text-danger"></span>
            </div>
            <input type="hidden" asp-for="Estelista.Id" />
            <div class="form-group">
                <label asp-for="Estelista.Nombre" class="control-label"></label>
                <input asp-for="Estelista.Nombre" class="form-control" />
                <span asp-validation-for="Estelista.Nombre" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Estelista.Apellidos" class="control-label"></label>
                <input asp-for="Estelista.Apellidos" class="form-control" />
                <span asp-validation-for="Estelista.Apellidos" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Estelista.Email" class="control-label"></label>
                <input asp-for="Estelista.Email" class="form-control" />
                <span asp-validation-for="Estelista.Email" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Estelista.Contrasena" class="control-label"></label>
                <input asp-for="Estelista.Contrasena" class="form-control" />
                <span asp-validation-for="Estelista.Contrasena" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Estelista.Celular" class="control-label"></label>
                <input asp-for="Estelista.Celular" class="form-control" />
                <span asp-validation-for="Estelista.Celular" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="Estelista.EstadoSalud" /> @Html.DisplayNameFor(model => model.Estelista.EstadoSalud)
                </label>
            </div><br>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" /> | <a class="btn btn-success" asp-page="./MenuEstilista">Regresar </a>
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
